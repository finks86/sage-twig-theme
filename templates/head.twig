<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    {{ function('wp_head') }}
    {% for post in posts %}
        <link rel="apple-touch-icon" sizes="180x180" href="{{ theme.link }}/dist/images/touch/{{ post.id }}/apple-touch-icon.png" />
        <link rel="shortcut icon" href="{{ theme.link }}/dist/images/touch/{{ post.id }}/android-chrome-192x192.png" sizes="196x196">
        <link rel="shortcut icon" href="{{ theme.link }}/dist/images/touch/{{ post.id }}/android-chrome-512x512.png" sizes="512x512">
        <link rel="icon" href="{{ theme.link }}/dist/images/touch/{{ post.id }}/favicon.ico" type="image/x-icon" />
    {% endfor %}
    <script>
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
                m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

        ga('create', 'UA-84828703-1', 'auto');
        ga('send', 'pageview');

    </script>
</head>
