{% extends "templates/base.twig" %}

{% block content %}

    <div class="section section--logo">
        <img src="{{ theme.link }}/dist/images/logo_next_big.png">
    </div>
    {% for page in pages %}

        {% include "templates/content.twig" with {post: page}  %}

    {% endfor %}

    <div class="section contact">
        <div class="container-fluid">
            <div class="form-container">
                <div class="title-container"><h2>Contact</h2></div>
                {{ contact }}
                <a href="" class="download-link"><span><i class="fa fa-arrow-circle-o-down" aria-hidden="true"></i>Download info PDF</span></a>
            </div>
        </div>
    </div>
    <div>
        {% for page in other_pages %}
            <div id="other-page-{{ page.id }}">
            <h2>{{ page.post_title }}</h2>
            <div><p>{{ page.post_content }}</p></div>
            </div>
        {% endfor %}
    </div>

{% endblock %}

{#{% block sidebar %}#}

    {#{% include "templates/sidebar.twig" %}#}

{#{% endblock %}#}